{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.15",
        "nelmio/cors-bundle": "^2.3",
        "phpoffice/phpspreadsheet": "^1.29",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/messenger": "6.3.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/runtime": "6.3.*",
        "symfony/yaml": "6.3.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.5",
        "qossmic/deptrac-shim": "^1.0",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/maker-bundle": "^1.51",
        "symfony/phpunit-bridge": "^6.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "db:truncate": [
            "./bin/console doctrine:database:drop --force",
            "./bin/console doctrine:database:create || true",
            "./bin/console doctrine:migrations:migrate --no-interaction"
        ],
        "phpunit": "php ./vendor/phpunit/phpunit/phpunit",
        "tests:unit": "@phpunit tests/Unit",
        "tests:application": [
            "@db:seed:tests",
            "@phpunit --testdox --colors tests/Application"
        ],
        "tests:architecture": "./vendor/bin/deptrac",
        "build": [
            "zip app.zip -r bin config resources migrations src vendor composer.json tools composer.lock symfony.lock public/index.php .env"
        ],
        "db:seed:tests": "php bin/console doctrine:fixtures:load --no-interaction --no-debug"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    }
}
